package fundamentos;

public class ConversaoTipoPrimitivoNumerico {
	
	public static void main(String[] args) {
		
		double a = 1;
		// conversão implícita (impossível perder informações)
		System.out.println(a);
		
		//CASTING (conversão de tipos primitivos)
		float b = (float) 1.123456788888; 
		// conversão explícita (Há probabilidade de perder informações)
		System.out.println(b);
	}
}
