package controle.exercicios;

import java.util.Scanner;

public class Exercicio09 {
	
	public static void main(String[] args) {
		//Crie um programa que recebe 10 valores e
		//ao final imprima o maior número
		
		Scanner entrada = new Scanner(System.in);
		
		int valorDigitado;
		int maior = 0;		
		
		for (int i = 1; i <= 10; i++) {
			System.out.printf("Insira o %dº valor: ", i);
			valorDigitado = entrada.nextInt();
			
		}
		
		System.out.printf("Maior número: %d", maior));

		entrada.close();
	}
}
