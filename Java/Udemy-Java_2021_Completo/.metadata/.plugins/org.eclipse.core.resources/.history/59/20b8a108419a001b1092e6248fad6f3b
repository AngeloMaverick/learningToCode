package classe;

public class ValorNulo {
	
	public static void main(String[] args) {
		
		String s1 = "";
		System.out.println(s1.concat("!!!!"));
		
		String s2 = null;
		System.out.println(s2.concat("????"));
		// NullPointerException = Erro de acesso a métodos ou atributos de 
		//um objeto nulo(sem endereço de memória). 
	}
}
