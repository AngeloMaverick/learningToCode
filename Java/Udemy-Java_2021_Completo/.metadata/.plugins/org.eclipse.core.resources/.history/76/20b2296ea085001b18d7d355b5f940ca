package fundamentos;

public class NotacaoPonto {
	public static void main(String[] args) {
		String s = "Bom dia X"; //String com S maiúsculo
		s = s.toUpperCase(); //Métodos não armazenam na variável em questão
		s = s.replace("X", "Senhora"); //É preciso atribuir
		
		System.out.println(s);
	}
}
